<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape>
            <solid android:color="@color/color_f0f0f0" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <solid android:color="@color/color_400bbe06" />
            <corners android:radius="20.0dip" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/color_400bbe06" />
            <corners android:radius="20.0dip" />
        </shape>
    </item>
    <item android:state_enabled="true">
        <shape>
            <solid android:color="@color/color_23d41e" />
            <corners android:radius="15.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="@color/color_400bbe06" />
            <corners android:radius="20.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="@color/color_0bbe06" />
            <corners android:radius="20.0dip" />
        </shape>
    </item>
</selector>