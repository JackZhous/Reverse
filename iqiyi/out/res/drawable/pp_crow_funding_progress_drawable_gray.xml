<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#fff0f0f0" android:endColor="#fff0f0f0" android:angle="270.0" android:centerColor="#fff0f0f0" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffcccccc" android:endColor="#ffcccccc" android:angle="0.0" android:centerX="0.75" android:centerColor="#ffcccccc" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffcccccc" android:endColor="#ffcccccc" android:angle="0.0" android:centerX="0.75" android:centerColor="#ffcccccc" />
            </shape>
        </clip>
    </item>
</layer-list>