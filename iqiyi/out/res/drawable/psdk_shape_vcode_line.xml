<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true" android:state_selected="false">
        <shape android:shape="line">
            <stroke android:width="1.0dip" android:color="#ffe6e6e6" />
        </shape>
    </item>
    <item android:state_enabled="true" android:state_selected="true">
        <shape android:shape="line">
            <stroke android:width="1.0dip" android:color="#ffeb3d3f" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="line">
            <stroke android:width="1.0dip" android:color="@color/psdk_configurable_edittext_cursor" />
        </shape>
    </item>
</selector>