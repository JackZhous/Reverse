<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="5.0dip" />
            <gradient android:startColor="#ffe6e6e6" android:endColor="#ffe6e6e6" android:angle="270.0" android:centerColor="#ffe6e6e6" />
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffff8533" android:endColor="#ffff6200" android:angle="0.0" />
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="5.0dip" />
                <gradient android:startColor="#ffff8533" android:endColor="#ffff6200" android:angle="0.0" />
            </shape>
        </clip>
    </item>
</layer-list>